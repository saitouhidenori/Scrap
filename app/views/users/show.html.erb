<div class="user_page">
    <header class="user_page-header">
      <div class="yjContainer">
        <a href="">
          <h2><%=  current_user.email%> <span>さんのマイページ</span></h2>
        </a>
      </div>
    </header>
    <div class="pt1em pb1em" id="contents">  <div id="main_cnt_wrapper">
      <div id="yjContentsBody">
        <div class="yjContainer">
          <ul class="user_page-contents">
            <%  current_user.likes.each do |like| %>
              <li style="background-image: url(<%= like.style.image %>)">
                <div class="meta">
                  <header>
                    <div class="title"><%= like.style.style %></div>
                    <!-- <span class="star"><span class="rating-star"><i class="star-actived rate-<%= review.rate %>0"></i></span></span> -->
                  </header>
                  <div class="user_review"><%= like.like %></div>
                </div>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
